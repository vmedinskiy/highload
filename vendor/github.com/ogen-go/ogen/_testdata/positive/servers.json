{
  "openapi": "3.0.3",
  "servers": [
    {
      "x-ogen-server-name": "production",
      "url": "https://{region}.example.com/{val}/v1",
      "description": "Production server",
      "variables": {
        "region": {
          "default": "us",
          "enum": [
            "us",
            "eu"
          ]
        },
        "val": {
          "default": "prod",
          "enum": [
            "prod",
            "test"
          ]
        }
      }
    },
    {
      "x-ogen-server-name": "prefix",
      "url": "/{val}/v1",
      "variables": {
        "val": {
          "default": "prod",
          "enum": [
            "prod",
            "test"
          ]
        }
      }
    },
    {
      "x-ogen-server-name": "const",
      "url": "https://cdn.example.com/v1"
    }
  ],
  "info": {
    "title": "title",
    "version": "v0.1.0"
  },
  "paths": {
    "/healthz": {
      "get": {
        "operationId": "probeLiveness",
        "description": "Liveness probe for kubernetes",
        "responses": {
          "200": {
            "description": "User info",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "default": {
            "$ref": "#/components/responses/Error"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Error": {
        "description": "Represents error object",
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int64"
          },
          "message": {
            "type": "string"
          }
        },
        "required": [
          "code",
          "message"
        ]
      }
    },
    "responses": {
      "Error": {
        "description": "General Error",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      }
    }
  }
}
